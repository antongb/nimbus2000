<a href="#/tracks/<%= track.id %>"><h4><%= track.escape('title') %></h4></a>

<h5>Uploaded by <a href="#/users/<%= track.escape('uploader_id') %>"><%= track.escape('uploader_name') %></a></h5>

<% if (track.get('cover_thumb')){ %>
  <img src="<%= track.escape('cover_thumb') %>" alt="<%= track.escape('title') %>" />
<% } %>


<% if (queue) { %>
  <audio class="player" src="<%= track.escape('audio') %>" preload="none">
    <p>Your browser does not support the audio element.</p>
  </audio>
<% } %>


